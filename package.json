{
  "name": "stl-parser",
  "version": "0.0.0",
  "description": "Minimal, practical boilerplate for writing Node-powered CLI tools",
  "main": "lib/index.js",
  "homepage": "https://github.com/jeremyckahn/stl-parser",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeremyckahn/stl-parser.git"
  },
  "bugs": {
    "url": "https://github.com/jeremyckahn/stl-parser/issues",
    "email": "AUTHOR_EMAIL@gmail.com"
  },
  "bin": {
    "stl-parser": "./bin/cli.js"
  },
  "scripts": {
    "build": "webpack",
    "test": "mocha ./node_modules/babel-core/register.js test/index.js",
    "test:debug": "mocha debug ./node_modules/babel-core/register.js test/index.js",
    "test:watch": "nodemon --exec \"npm test\" --watch lib --watch test",
    "lint": "eslint lib test webpack.*",
    "precommit": "pretty-quick --staged && npm test && npm run lint",
    "prepush": "npm test && npm run lint",
    "prettier": "prettier '{lib,test}/**/*.js' --write",
    "preversion": "npm test",
    "postversion": "git push && git push --tags && npm publish",
    "exec": "./bin/cli.js"
  },
  "author": "jeremyckahn@gmail.com",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.10.0",
    "get-stdin": "^6.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.5.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  },
  "files": [
    "dist",
    "bin",
    "lib"
  ]
}
